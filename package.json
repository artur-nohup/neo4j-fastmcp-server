{
  "name": "neo4j-fastmcp-server",
  "version": "1.0.0",
  "description": "Neo4j MCP Server with Descope authentication for Claude.ai integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "tsx src/test.ts",
    "test:inspector": "tsx src/test-inspector.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "neo4j",
    "fastmcp",
    "claude",
    "anthropic",
    "oauth",
    "descope",
    "authentication",
    "knowledge-graph",
    "ai",
    "llm"
  ],
  "author": "Artur Renzenbrink",
  "license": "MIT",
  "dependencies": {
    "@descope/node-sdk": "^1.19.0",
    "fastmcp": "^0.1.6",
    "neo4j-driver": "^5.26.0",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "jose": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/cookie-parser": "^1.4.7",
    "@types/express-session": "^1.18.0",
    "typescript": "^5.6.3",
    "tsx": "^4.19.2",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artur-nohup/neo4j-fastmcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/artur-nohup/neo4j-fastmcp-server/issues"
  },
  "homepage": "https://github.com/artur-nohup/neo4j-fastmcp-server#readme"
}